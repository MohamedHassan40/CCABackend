{
  "name": "cca-backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:with-db": "npx prisma migrate deploy && npx prisma db seed && node dist/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx src/core/db/seed.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:modules": "vitest run tests/modules",
    "test:integration": "vitest run tests/integration",
    "test:unit": "vitest run tests/unit"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@sendgrid/mail": "^8.1.6",
    "@types/multer": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "uuid": "^13.0.0",
    "@sentry/node": "^7.91.0",
    "bullmq": "^5.1.0",
    "ioredis": "^5.3.2",
    "prisma": "^5.7.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^1.1.3",
    "supertest": "^6.3.3",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "prisma": {
    "seed": "tsx src/core/db/seed.ts"
  }
}